{
  "plugins": [
    "import",
    "prettier",
    "react"
  ],
  "parser": "babel-eslint",
  "extends": [
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "warn"
  },
  "settings": {
    "react": {
      "version": "17.00.0"
    },
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    },
    "webpack": {
      "config": "webpack.config.babel.js"
    }
  }
}
